@{
    ViewBag.Title = "Home Page";
}

<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var ulEmployees = $('#ulEmployees');

        $('#btn').click(function () {
            $.ajax({
                type: 'GET',
                // Make sure to change the port number to
                // where you have the employee service
                // running on your local machine
                url: 'http://localhost:56748/api/Customer/GetAllCustomers',
                dataType: 'json',
                headers: {
                    'Authorization': 'hardik4610'
                },
                success: function (data) {
                    ulEmployees.empty();
                    $.each(data, function (index, val) {
                        ulEmployees.append('<li>' + val.customerName + '</li>')
                    });
                }
            });
        });

        $('#btnClear').click(function () {
            ulEmployees.empty();
        });
    });
</script>

<input id="btn" type="button" value="Get All Customers" />
<input id="btnClear" type="button" value="Clear" />
<ul id="ulEmployees"></ul>